{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "cSpell.words": [
    "Chakra",
    "codegen",
    "Formik",
    "middlewares",
    "resave",
    "timestamptz",
    "typeorm"
  ],
  "security.workspace.trust.enabled": false
}
